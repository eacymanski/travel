<h1>New Destination</h1>
<%=form_for(@destination) do |f| %>
  <div class="field">
    <%= f.label :city %><br>
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <%= f.label :state %><br>
    <%= f.select :state, Destination::STATES %>
  </div>
  <%= f.hidden_field :trip_id, value: @current_trip %>
  <div class="actions">
    <%= f.submit %>
  </div>
<%end%>
<%= link_to 'Back', trip_path(@current_trip) %>
